CREATE (:DESCRIPTION {zh_title: "忠诚", hi_title: "वफ़ादारी", es_title: "FIDELIDAD", fr_content: "Adhésion à quelque chose à laquelle on est lié par un engagement ou un devoir.", hi_content: "किसी ऐसी चीज़ का पालन करना जिसके प्रति कोई प्रतिज्ञा या कर्तव्य से बंधा हो।", en_title: "FAITHFULNESS", es_content: "Adherencia a algo a lo que uno está obligado por una promesa o deber.", name: "desc.FAITHFULNESS", en_content: "Adherence to something to which one is bound by a pledge or duty.", fr_title: "FIDÉLITÉ", zhi_content: "坚守承诺或义务所约束的事物。"});
